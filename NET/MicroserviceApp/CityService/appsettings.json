{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=mydb;Username=user;Password=password"
  },
  "spring" : {
    "application" : {
      "name" : "cityservice"
    }
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://eureka:8080/eureka/",
      "ValidateCertificates":  false
    },
    "instance" : {
      "appName" : "cityservice",
      "hostName" : "cityservice",
      "port" : 3001
    }
  }
}
